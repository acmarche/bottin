<div class="hidden-print mb-3">
    {{ form_start(search_form, { 'attr': {'class': 'form-inline'}}) }}
    {{ form_errors(search_form) }}
    <div class="row">
        <div class="col">
            {{ form_row(search_form.nom, {label_attr: {class: 'sr-only'}, attr:{'class': ' my-1 mr-sm-2'} }) }}
        </div>

        <div class="col">
            {{ form_row(search_form.localite, {label_attr: {class: 'sr-only'}, attr:{'class': ' my-1 mr-sm-2'} }) }}
        </div>
    </div>

    <button class="btn btn-primary mr-1" type="submit"><i class="fas fa-search" aria-hidden="true"></i>
        Rechercher
    </button>

    {{ form_rest(search_form) }}
    {{ form_end(search_form) }}
</div>
